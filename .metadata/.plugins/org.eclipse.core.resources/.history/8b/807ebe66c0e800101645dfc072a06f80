package br.uff.tempo.nanicolina.devices;

public class Local {
	
	private int x;
	private int y;
	
	public Local(int x, int y)
	{
		this.x = x;
		this.y = y;		
	}
	
	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}
	
	public String toString()
	{
		return 
	}

}
